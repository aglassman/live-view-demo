<div
    class="blokus"
    phx-click="toggle" phx-window-keydown="move"
    >
   <%= for n <- 1..400 do %>
    <div
        phx-click="toggle" phx-value-index="<%= n %>"
        style="<%= case @block_map[n] do
                        {:user, name, color} -> "background-color: #{color}"
                        _ -> ""
                    end
                %>"
        ></div>
   <% end %>
</div>
<p><%= user_names %{} %></p>